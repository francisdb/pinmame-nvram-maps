# EditorConfig is awesome: https://editorconfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# Standardized format for maps.  See normalize-map.sh for jq script to reformat a file.
[*.json]
indent_style = space
indent_size = 2
